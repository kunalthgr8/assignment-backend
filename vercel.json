{
    "version": 2,
    "builds": [
      { "src": "manage.py", "use": "@vercel/python" },
      { "src": "static/**/*", "use": "@vercel/static" },
      {
        "src": "api/**/*.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "manage.py" }
    ]
  }
  